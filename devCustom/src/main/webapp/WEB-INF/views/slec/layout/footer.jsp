	                        var numb = this.activeIndex;
	                        var s = $("#swipelistid .swiper-slide").eq(numb).find("img.imgblk.rs").attr('src');
	                        if (typeof s == "undefined" || s == null || s == "") {
	                            return;
	                        }
	                        var bs = s.replace(/\/([^/]+)-[^.]+\.(jpg|png|jpeg|webp|JPG)/, "/$1-resize.$2");
